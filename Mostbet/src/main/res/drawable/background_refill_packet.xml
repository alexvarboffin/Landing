<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="false">
        <shape android:shape="rectangle">
            <corners android:radius="8.0dip" />
            <stroke android:width="1.0dip" android:color="?colorSupportContactDivider" />
            <solid android:color="?colorRegisterBonusBackground" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <corners android:radius="8.0dip" />
            <stroke android:width="1.0dip" android:color="@color/color_yellow_orange" />
            <solid android:color="?colorRegisterBonusSelectedBackground" />
        </shape>
    </item>
</selector>